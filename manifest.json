{
  "manifest_version": 3,
  "name": "AZ AI Helper",
  "version": "0.1",
  "author": "Pulkit Darwani",
  "description": "A chatbot ai helper that helps you solving problems on algozenith",
  "permissions": [
    "storage","tabs"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://maang.in/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources" : [
    {
      "resources": [
        "assets/bookmark.png",
        "assets/delete.png",
        "assets/play.png",
        "inject.js"
      ],
      "matches" : ["<all_urls>"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "assets/ext-icon.png",
      "24": "assets/ext-icon.png",
      "32": "assets/ext-icon.png"
    },
    "default_popup": "popup.html",
    "default_title": "AZ AI Helper"
  }
}
